%\r\n      );\r\n}\r\n`;\r\nlet string2=''\r\nlet n=0;\r\nlet step=()=>{\r\n    setTimeout(()=>{ \r\n        \r\nif(string[n]==='\\n'){\r\nstring2=string2+'<br>';\r\n        }else if(string[n]===' '){\r\n            string2=string2+'&nbsp';\r\n        }else{\r\n            string2=string2+string[n];\r\n        }\r\nhtml.innerHTML=string2;\r\nstyle.innerHTML=string.substring(0,n);\r\nwindow.scrollTo(0,99999);\r\nhtml.scrollTo(0, 99999);\r\n   if(n<string.length-1){\r\n       n=n+1;\r\n       step();\r\n   }\r\n},0);\r\n};\r\nstep();\r\n\r\n"},"lineCount":null}},"error":null,"hash":"f13ff0a5c2d3f2d1802717e89c903e9f","cacheData":{"env":{}}}